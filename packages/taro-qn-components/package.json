  "license": "ISC",
  "browser": "dist/index.js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "components/index.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
